package zoo_Keeper;

public class Bat extends Mammal
{
	public Bat()
	{
		energyLevel=300;
	}
	
	void fly()
	{
		System.out.println("Whoosh Whoosh Whoosh");
		engergyLevel-=50;
	}
	
	void eatHumans()
	{
		System.out.println("Crunch Crunch Crunch");
		engergyLevel+=25;
	}
	
	void attackTown()
	{
		System.out.println("Sounds of burning fire.");
		engergyLevel-=100;
	}
};
